{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended",
    "eslint:recommended"
  ],
  "plugins": [],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "spaced-comment": 0,
    "no-console": 1,
    "consistent-return": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "no-process-exit": 0,
    "no-param-reassign": 0,
    "no-return-await": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "prefer-destructuring": ["error", { "object": true, "array": false }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "req|res|next|val" }]
  }
}
